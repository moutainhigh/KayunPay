    color: #333;
    line-height: 65px;
    font-size: 16px;
    margin: 0 20px;
    height:60px;
}
.banner .data .a_panel a{
	position: static;
	display: inline;
}
.banner .data li .red {
  float: right;
  margin-right: 10px;
  width: 110px;
}

.banner a{
	position: absolute;
	display: none;
	width: 100%;
	height: 100%;
	z-index:2
	/*
	opacity: 0;
	-ms-opacity: 0;
	-moz-opacity: 0;
	-webkit-opacity: 0;
	-o-opacity: 0;
	*/
}

.banner .buttons{
	position: absolute;
	bottom: -37px;
	text-align: center;
	padding:6px 10px;
	left:370px;
    border-radius: 15px;
	z-index:2
}

.banner .buttons li{
	float: left;
