	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	-o-border-radius: 5px;
	-ms-border-radius: 5px;
}
.banner .data  .button_panel .button:hover{
	background:#FC4B4B
}
.banner .data  .button_panel .button i{
	width: 17px;
    height: 20px;
    background: url(/portal/images/index/ico.png);
    background-position: -168px 0;
    display: inline-block;
    margin-right: 8px;
    position: relative;
    top: 5px;
}
.banner .data .a_panel{
	margin: 22px 20px 0px;
	padding-right: 10px;
	font-size: 12px;
	color:#000
}
.banner .data .a_tips{
