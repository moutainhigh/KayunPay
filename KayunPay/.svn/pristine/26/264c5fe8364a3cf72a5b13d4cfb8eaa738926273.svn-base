	overflow: hidden;
}

.banner .layout_center{
	position: relative;
	margin-top: 330px;
	height: 20px;
}

.banner .data{
	position: absolute;
  top: -292px;
  right: 0px;
  width: 300px;
  height: 285px;
  text-align: center;
  color: #fff;
  z-index: 2;
}
.banner .data .user_data{
	padding: 46px 20px 0px;
	height: 82px;
}
.banner .data .indexUserName{
	font-style:normal;
}
.banner .data .user_data img{
	float: left;
	width: 82px;
	height: 82px;
	
	border-radius: 41px;
	-webkit-border-radius: 41px;
	-ms-border-radius: 41px;
	-o-border-radius: 41px;
	-moz-border-radius: 41px;
}

.banner .data a{
	position: static;
	display: inline;
	color: #347be7;
}

.banner .data .user_data div{
	margin-left: 100px;
	margin-bottom: 8px;
	font-size: 14px;
	font-weight: bolder;
	line-height: 20px;
}

.banner .data ul{
	margin-top: 20px;
	padding-left: 40px;
}

.banner .data li{
	font-size: 14px;
	line-height: 40px;
}

.banner .data .bg{
	position: absolute;
	left: 0px;
	top: 0px;
	width: 300px;
	height: 320px;
	background: #fff;
	z-index: -1;
	opacity: 0.8;
	-ms-opacity: 0.8;
	-webkit-opacity: 0.8;
	-o-opacity: 0.8;
	-moz-opacity: 0.8;
	border-radius:5px
}

.banner .data h5{
    margin-top: 40px;
    color: #000;
    font-size: 16px;
    font-weight: 100;
    text-align: center;
}

.banner .data .money_panel{
    font-size: 28px;
    color: #FF5254;
}

.banner .data .money{
	font-size: 65px;	
}

.banner .data .percent_panel{
	font-size: 12px;
}

.banner .data  .button_panel{
	margin: 10px 20px 0px;
}

.banner .data  .button_panel .button{
	display: block;
	border: 0px none;
	width: 100%;
	height: 45px;
	font-size: 16px;
	background: #FF5256;
	color: #ffffff;
	line-height: 42px;
	cursor: pointer;
	text-align: center;
