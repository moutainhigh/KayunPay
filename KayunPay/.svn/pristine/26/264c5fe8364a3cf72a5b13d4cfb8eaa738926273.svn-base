}
.list_table1_main .marquee ul{
	 animation: marquee 20s infinite 3s;
	-webkit-animation: marquee 15s infinite 3s;  
}
/*  @keyframes marquee{
    0%{transform: translateY(0px);}
   	20%{transform: translateY(0px);}
    37.5%{transform: translateY(-30px);}
    50%{transform: translateY(-30px);}
    62.5%{transform: translateY(-60px);}
    75%{transform: translateY(-60px);}
    87.5%{transform: translateY(-90px);}
    100%{transform: translateY(-90px);}
}
@-webkit-keyframes marquee{
	0%{transform: translateY(0px);}
    12.5%{transform: translateY(0px);}
    25%{transform: translateY(0px);}
    37.5%{transform: translateY(-30px);}
    50%{transform: translateY(-30px);}
    62.5%{transform: translateY(-60px);}
    75%{transform: translateY(-60px);}
    87.5%{transform: translateY(-90px);}
    100%{transform: translateY(-90px);}
}   */
@keyframes marquee{
    0%{transform: translateY(0px);}
    16.333% {transform: translateY(-30px);}
	33.333% {transform: translateY(-30px);}
	49.666% {transform: translateY(-60px);}
	66.666% {transform: translateY(-60px);}
	83% {transform: translateY(-90px);}
	100% {transform: translateY(-90px);}
}
@-webkit-keyframes marquee{
    0%{transform: translateY(0px);}
    16.333% {transform: translateY(-30px);}
	33.333% {transform: translateY(-30px);}
	49.666% {transform: translateY(-60px);}
	66.666% {transform: translateY(-60px);}
	83% {transform: translateY(-90px);}
	100% {transform: translateY(-90px);}
}   


