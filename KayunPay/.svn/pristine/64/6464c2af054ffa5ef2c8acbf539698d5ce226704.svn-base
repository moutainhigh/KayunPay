.popOver {
  position: absolute;
  top: -50px;
  z-index: 1010;
  /* display: none; */
  max-width: 300px;
  padding: 1px;
  white-space: normal;
  background-color: #fff;
  -webkit-background-clip: padding-box;
  background-clip: padding-box;
  border: 1px solid #ccc;
  border: 1px solid #f04646;
  border-radius: 3px; 
  -webkit-box-shadow: 0 5px 10px rgba(0,0,0,.2);
  box-shadow: 0 5px 10px rgba(0,0,0,.2);
}
.popOver .popArrow {
	width: 0;
	height: 0;
	border-width: 6px 6px 0 6px;
	border-style: solid;
	border-color: #f04646 transparent transparent;
	position: absolute;
	left: 50%;
	bottom: -6px;
	margin-left: -6px;
	margin-top: -10px;
}
.popOver .popArrow:after {
	content: '';
	width: 0;
	height: 0;
	border-width: 6px 6px 0 6px;
	border-style: solid;
	border-color: #fff transparent transparent;
	position: absolute;
	left: 50%;
	bottom: 1px;
	margin-left: -6px;
	margin-top: -10px;
}
.popOver .popContent{
  padding: 10px 36px;
  color: red;
  
	line-height: normal !important;
	font-size: 12px !important;
}
.forNewRule{
	position: absolute;
  top: -70px;
  left:-125px;
  z-index: 1010;
  display: none; 
  max-width: 300px;
  padding: 1px;
  white-space: normal;
  background-color: #fff;
  -webkit-background-clip: padding-box;
  background-clip: padding-box;
  border: 1px solid #ccc;
  border: 1px solid #048dd1;
  border-radius: 3px; 
  -webkit-box-shadow: 0 5px 10px rgba(0,0,0,.2);
  box-shadow: 0 5px 10px rgba(0,0,0,.2);
}
.forNewRule .forNewRuleArrow {
	width: 0;
	height: 0;
	border-width: 6px 6px 0 6px;
	border-style: solid;
	border-color: #048dd1 transparent transparent;
	position: absolute;
	left: 50%;
	bottom: -6px;
	margin-left: -6px;
	margin-top: -10px;
}
.forNewRule .forNewRuleArrow:after {
	content: '';
	width: 0;
	height: 0;
	border-width: 6px 6px 0 6px;
	border-style: solid;
	border-color: #fff transparent transparent;
	position: absolute;
	left: 50%;
	bottom: 1px;
	margin-left: -6px;
	margin-top: -10px;
}
.forNewRule .forContent{
	min-width: 200px;
	padding: 10px 36px;
  	color: #048dd1;  
	line-height: normal !important;
	font-size: 12px !important;
}

