<script>
YRHX.ajax({
	url : CONFIG.getRequestURI("duangToubiaoOrder1"),
    success: function (sucData) {
    	var overHtml="<tr>";
    	overHtml += "<td><i></i></td>";
    	overHtml += "<td>#{userName}</td>";
    	/* overHtml += "<td><strong>#{tzje}</strong></td>"; */
    	overHtml += "<td>#{tbbs}</td>";
    	overHtml += "<td>#{pingjun_rate}%</td>";
    	/* overHtml += "<td>#{userScore}</td>"; */
    	overHtml +="</tr>";
    	$('.all_list').find('tbody').html('');
    	if(sucData && sucData.length > 0){
    		for(var i=0; i< sucData.length; i++){
    			var row=sucData[i];
    			var len=row["userName"].length;
    			if( len <2){
    				row["userName"]=row["userName"] + "***";
    			}else if(len < 8){
    				row["userName"]=row["userName"].substr(0,Math.floor(len / 2)) +"***";
    			}else{
    				row["userName"]= row["userName"].substr(0,3) +"***"
    			}
    			 row["tzje"] = YRHX.toDecimal2(row["tzje"] / 10.0/10.0, "￥"); 
    			/* row["userScore"] = (row["userScore"]/10.0/10.0).toFixed(2); */
    			row["pingjun_rate"] = row["pingjun_rate"] / 10.0 / 10.0;
    			var s = row["pingjun_rate"] + "";
    			row["pingjun_rate"] = s.substring(0,5);
    			var tempHTml=overHtml.makeHtml(row);
    			$('.all_list').find('tbody').append(tempHTml);
    		}
    	}else{
    		
    	}
    	$.rankICon('.all_list tr');
    	
    },
    error: function (errData) {
	}
});

//月、日排行
