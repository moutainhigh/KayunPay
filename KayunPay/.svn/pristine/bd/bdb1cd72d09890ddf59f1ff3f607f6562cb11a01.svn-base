					var bankcity=sucData[i];
					city.append("<option value='"+bankcity.cityCode+"'>"+bankcity.cityName+"</option>");}
				},
				error:function(errorData){
				$.popTips("popTipErr",errorData.return_info || "获取信息失败");
			}
			});
			}
			
		});
	}
