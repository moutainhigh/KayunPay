				city.html("");
				city.append("<option value='请选择城市' selected='selected'>请选择城市</option>");
			}else{
			YRHX.ajax({
				url:"/queryBankCityCodes",
				data:{"provinceCode":provinceCode},
				success:function(sucData){
					city.html("");
