	<script>
	var deposit_mobile = "";
	var verifyAuth=YRHX.queryString("verifyAuth");
	if(verifyAuth=="y"){
		YRHX.Cookie("verifyAuth",verifyAuth).set();
	}
	$(function(){
		//日期
		$('#datepicker').datepicker({
	           language:"zh-CN",    //语言选择中文
	           format:"yyyy-mm-dd",    //格式化日期
	           timepicker:true,     //关闭时间选项
	           yearEnd:2050,        //设置最大年份
	           autoclose: 1,        //选择完日期后，弹出框自动关闭
	           minView: 3,          //能选择到的最小日期格式
	    });
		
		
	});
	
	var assess = "";
	    setting(3, window.userNavActive);
		NavMenu(3);
		
		$("#mobileBox .desTitle").text("您绑定的平台手机号为"+YRHX.Cookie("loginName").get());
		if(YRHX.Cookie("loginId").get()){
			$("#mobileBox .safeitleRight3").text("修改绑定存管手机号");
		}else{
			$("#mobileBox .safeitleRight3").text("")
		}
		function closeModal() {
			$("#yinying").hide()
			$(".popModal").hide()
			$("html,body").css("overflow", "initial")
		}

		function showModal() {
			$("#yinying").show()
			$(".popModal").show()
			$('body,html').css("overflow", "hidden");
		}
		

		$(".safeitleRight").click(function () {
			if (!$(this).hasClass('isDone')) {
				$(".safeCenterItem").hide()
				switch ($(this).attr('optype')) {
					/* case 'realIdenBind':
						showModal();
						$(".modalTitle").hide();
						window.location.href="depositoryUser";
						break; */
					case 'cfcaBind':
						var isAgree = confirm('是否已阅读并同意电子签章授权认证！');
						if (isAgree) {
							YRHX.ajax({
								url: CONFIG.getRequestURI("userCFCA"),
								success: function (sucData) {
									window.location.reload()

								},
								error: function () {
									$.popTips("popTipErr", errData.return_info);
								}
							})
						}
						// $(".modalTitleDes").text("电子签章授权认证")
						// $('#realIdenModal').show();
						break;
					case 'mailIdenModal':
						showModal()
						$(".modalTitleDes").text("邮箱认证")
						$('#mailIdenModal').show();
						break;
					case 'loginPwdModal':
						showModal()
						$(".modalTitleDes").text("登录密码修改")
						$('#loginPwdModal').show();
						break;
					case 'payPwdModal':
						showModal()
						$(".modalTitleDes").text("支付密码设置")
						$('#payPwdModal').show();
						break;
					/* case 'payPwdModal1':
						showModal()
						$(".modalTitleDes").text("存管交易设置")
						$('#payPwdModal1').show();
						break; */
					case 'cardBind':
						//window.location.href = '/B04'
						showModal();
						$(".modalTitle").hide();
						//to_center($('#addBankDialog').show());
						window.location.href="depositoryUser?step=1";
						break;
					case 'mobileModal':
						showModal()
						$(".modalTitleDes").text("手机号修改")
						$('#mobileModal').show();
						break;
					case 'openPayment':
						showModal()
						$(".modalTitleDes").text("立即授权")
						$('#openPayment').show();
						break;
					case 'openRepay':
						showModal()
