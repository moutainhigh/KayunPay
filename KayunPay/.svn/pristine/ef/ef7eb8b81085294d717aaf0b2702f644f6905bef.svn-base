	$.queryInvest4MyA = function( indexPage ){
		YRHX.ajax({
			url : CONFIG.getRequestURI("queryInvest4Latest"),
			data : {
				"pageNumber" : indexPage || 1,
				"pageSize" : 3
			},
			success : function(sucData) {
		
				var rowHtml = "<tr>" ;
				rowHtml = rowHtml + "<td>#{loanDateTime}</td>";
				rowHtml = rowHtml + "<td>#{payAmount}</td>";
				rowHtml = rowHtml + "<td><span>#{leftInterest}</span></td>";
				rowHtml = rowHtml + "</tr>";

				var returnData = sucData.list ;

				if ( returnData && returnData.length > 0) {
					$("#recentLoanPage .uArrowRight").attr({
						"indexPage":sucData.pageNumber + 1,
						"totalPage":sucData.totalPage
					});

					$("#recentLoanPage .uArrowLeft").attr({
						"indexPage":sucData.pageNumber -1,
						"totalPage":sucData.totalPage
					});

					if( sucData.firstPage ){
						$("#recentLoanPage .uArrowRight").addClass("c");
					}else{
						$("#recentLoanPage .uArrowRight").removeClass("c");
					}

					if( sucData.lastPage ){
						$("#recentLoanPage .uArrowLeft").addClass("c");
					}else{
						$("#recentLoanPage .uArrowLeft").removeClass("c");
					}

					if( !sucData.firstPage && !sucData.lastPage){
						$("#recentLoanPage .uArrowRight").addClass("c");
						$("#recentLoanPage .uArrowLeft").addClass("c");
					}
					
					if( sucData.firstPage && sucData.lastPage){
						$("#recentLoanPage .uArrowRight").removeClass("c");
						$("#recentLoanPage .uArrowLeft").removeClass("c");
					}
					
					var tableEle = $("#recentLoan").find("tbody");
			
					tableEle.html("");
					
