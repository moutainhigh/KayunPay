	});
	$('.pop_i').click(function(){
		$(this).addClass("pop_ii").siblings().removeClass("pop_ii");
		var index = $('.pop_i').index(this);
		if(index==1){
			$('.content_p3').attr("names","A");
			$('.content_p3').attr("half","1");
			$('.zzxy').hide();
		}else if(index==3){
			$('.content_p3').attr("names","B");
			$('.content_p3').attr("half","1");
			$('.zzxy').hide();
		}
		else if(index==0){
			$('.content_p3').attr("names","A");
			$('.content_p3').removeAttr("half");
			$('.jumpxy').attr('href','/xy1');
			$('.xy').attr('checked',false);
			$('.zzxy').show();
		}else if(index==2){
			$('.content_p3').attr("names","B"); 
			$('.content_p3').removeAttr("half");
			$('.jumpxy').attr('href','/xy2');
			$('.xy').attr('checked',false);
			$('.zzxy').show();
		}
		else if(index==4){
			$('.content_p3').attr("names","C");
			$('.content_p3').attr("half","1");
			$('.jumpxy').attr('href','/xy3');
			$('.xy').attr('checked',false);
			$('.zzxy').show();
		}else if(index==5){
			$('.content_p3').attr("names","D"); 
			$('.content_p3').removeAttr("half");
			$('.zzxy').hide();
		}
		else if(index==6){
			$('.content_p3').attr("names","E");
			$('.content_p3').removeAttr("half");
 			var  tzHtml  = `
			 风险提示告知书
			 致出借人：
			 鉴于您选择债权调整中方案五，现将有关后果和可能风险告知您如下：
			 1、借款人正常按期还款，平台按借款人还款情况定期向您回款；
			 2、借款人逾期还款的，逾期风险由您自行承担，与平台无关；
			 3、借款逾期还款的，由平台进行法催，法催费用由您自行承担（包括但不限于诉讼费、律师费、保全费、差旅费等），回款时间视借款人还款时间而定。`
 	      	alert(tzHtml)
			$('.zzxy').hide();
		}else{
			$('.content_p3').attr("href","javascript:;'"); 
			$('.content_p3').removeAttr("half");
			$('.zzxy').hide();
		}
	});
