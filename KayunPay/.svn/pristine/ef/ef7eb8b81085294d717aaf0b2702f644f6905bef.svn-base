			var sucData = data["list"];
			var filter_this = [];
			var year = filter_date.substr(0,4);
			var month = filter_date.substr(4,2);
			var mlength = getDateLengthInMonth(year,month);
			$('.reback_allAmount4Month i').text(data['monthBenXi']);
